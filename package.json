{
  "name": "@actionlogementservices/aurelia-plugin-ui",
  "version": "2.1.0",
  "description": "An aurelia plugin repository that provides common bootstrap 5 ui components.",
  "keywords": [
    "aurelia",
    "plugin",
    "bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/actionlogementservices/aurelia-plugin-ui.git"
  },
  "license": "MIT",
  "author": "KisssDev",
  "main": "dist/commonjs/index.js",
  "module": "dist/native-modules/index.js",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "au build-plugin --env prod",
    "doc": "jsdoc -c ./.jsdoc.json",
    "prepare": "npm run build && npx tsc -p ts-d-generation.json && cpx src/types.d.ts dist/types",
    "release": "release-it",
    "start": "au run",
    "pretest": "au lint",
    "test": "au test",
    "update:browserslist": "browserslist --update-db",
    "upgrade": "npx npm-upgrade",
    "preversion": "au test",
    "watch": "au build-plugin --watch"
  },
  "browserslist": [
    ">= 0.2% in FR, last 5 versions, not dead, not Firefox 12"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "setupFiles": [
      "<rootDir>/test/jest-pretest.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.spec.js"
    ],
    "transform": {
      "^.+\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-class-static-block": "^7.26.0",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.4.0",
    "@types/geojson": "^7946.0.15",
    "@types/jest": "^29.5.14",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.4.1",
    "aurelia-cli": "^3.0.4",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^3.0.0-rc.2",
    "aurelia-testing": "^1.1.0",
    "aurelia-validation": "^2.0.0",
    "babel-jest": "^29.7.0",
    "connect": "^3.7.0",
    "connect-history-api-fallback": "^2.0.0",
    "connect-injector": "^0.4.4",
    "cpx": "^1.5.0",
    "debounce": "^2.2.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-array-func": "^5.0.2",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "gulp": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-eslint-new": "^2.4.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-watch": "^5.0.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jsdoc": "^4.0.4",
    "jsdoc-escape-at": "^1.0.1",
    "kis-jsdoc-plugin": "^2.2.0",
    "minimatch": "^10.0.1",
    "open": "^8.4.2",
    "prettier": "^3.4.2",
    "promise-polyfill": "^8.3.0",
    "regenerator-runtime": "^0.14.1",
    "release-it": "^17.11.0",
    "requirejs": "^2.3.7",
    "serve-static": "^1.16.2",
    "server-destroy": "^1.0.1",
    "socket.io": "^4.8.1",
    "text": "requirejs/text"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "chokidar": "^3.0.0",
    "glob-stream": "^7.0.0",
    "glob-parent": "^6.0.0",
    "micromatch": "^4.0.0"
  },
  "dependencies": {
    "core-js": "^3.40.0",
    "inputmask": "^5.0.9",
    "vanillajs-datepicker": "^1.3.4"
  },
  "peerDependencies": {
    "aurelia-configuration": "^2.0.0",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-validation": "^2.0.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3"
  }
}
