{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./src"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "opts": {
    "template": "node_modules/kis-jsdoc-plugin/templates/markdown",
    "encoding": "utf8",
    "destination": "./doc/",
    "recurse": true,
    "includes": "public,private"
  },
  "plugins": [
    "node_modules/jsdoc-escape-at",
    "node_modules/kis-jsdoc-plugin"
  ],
  "templates": {
    "markdown": {
      "imageext": "png",
      "tocfilename": "toc.md",
      "tocOrder": {
        "public": 0,
        "dialog": 1,
        "lock": 2,
        "toast": 3,
        "element": 10,
        "select": 11,
        "autocomplete": 12,
        "table": 13,
        "attribute": 20,
        "valueconverter": 30,
        "internal": 100
      },
      "badgecolors": {
        "public": "FF5000",
        "dialog": "009663",
        "lock": "009663",
        "toast": "009663",
        "element": "3b631b",
        "select": "3b631b",
        "autocomplete": "3b631b",
        "table": "3b631b",
        "attribute": "b16502",
        "valueconverter": "085a8a",
        "internal": "6b6b6b",
        "other": "9f9f9f"
      },
      "externallinks": {
        "Promise": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Promise",
        "FrameworkConfiguration": "https://aurelia-1.gitbook.io/v1-docs/developer-guides/building-plugins/plugin-development"
      }
    }
  }
}